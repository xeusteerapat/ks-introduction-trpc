generator client {
  provider = "prisma-client-js"
  output   = "../node_modules/.prisma/client"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Notes {
  id        Int      @id @default(autoincrement())
  title     String
  content   String
  createdAt DateTime @db.Timestamp()
  updatedAt DateTime @db.Timestamp()
}
